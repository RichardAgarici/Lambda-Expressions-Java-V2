@FunctionalInterface
interface MyLambda {
    void display();
}

/*
class My implements MyLambda{
    public void display(){
        System.out.println("Hello World");
    }
}
 */

public class LambdaDemo {
    public static void main(String[] args) {
        MyLambda m = () -> {
            System.out.println("Hello World");
        };
    }
}
